{
  "name": "react-native-gm-ev-charger-module",
  "version": "0.1.0",
  "description": "EV Charger module with TMap implementation for GM Motors",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-gm-ev-charger-module.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "add-peers": "npx npm-install-peers"
  },
  "keywords": ["react-native", "ios", "android"],
  "repository": "https://github.com/nerdyfactory/gm-ev-charger-module.git",
  "author": "NerdyFactory <supernerd@nerdyfactory.com> (https://github.com/nerdyfactory)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nerdyfactory/gm-ev-charger-module.git/issues"
  },
  "homepage": "https://github.com/nerdyfactory/gm-ev-charger-module.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "^0.18.1",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3",
    "@react-native-async-storage/async-storage": "^1.15.4"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.15.4",
    "react": "^16.13.1",
    "react-native": "^0.63.4",
    "react-native-geolocation-service": "^5.3.0-beta.1",
    "react-native-permissions": "^3.0.3",
    "react-router-native": "^5.2.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": ["@react-native-community", "prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false,
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": ["node_modules/", "lib/"],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
